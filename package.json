{
  "name": "pondera",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "lint": "eslint . index.js",
    "lint:fix": "eslint --fix . index.js",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "model:generate": "node_modules/.bin/sequelize model:generate",
    "db:create": "node_modules/.bin/sequelize db:create",
    "db:drop": "node_modules/.bin/sequelize db:drop",
    "seed:create": "node_modules/.bin/sequelize seed:create --name",
    "db:seed": "node_modules/.bin/sequelize db:seed",
    "db:seed:all": "node_modules/.bin/sequelize db:seed:all"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "crypto": "1.0.1",
    "dotenv": "4.0.0",
    "express": "~4.13.1",
    "google-auth-library": "^0.11.0",
    "googleapis": "^22.2.0",
    "nodemon": "^1.12.1",
    "parameters-middleware": "",
    "pg": "^7.4.0",
    "pg-promise": "~7.0.3",
    "randomstring": "",
    "sequelize": "^4.22.6",
    "sequelize-cli": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0"
  }
}
